{
  "name": "analogger",
  "version": "1.0.0",
  "description": "Js Logger",
  "main": "src/cjs/ana-logger.cjs",
  "module": "src/esm/ana-logger.mjs",
  "type": "module",
  "exports": {
    ".":{
      "require": "./src/cjs/ana-logger.cjs",
      "import": "./src/esm/ana-logger.mjs"
    }
  },
  "scripts": {
    "demo": "node example/cjs/demo-1.cjs",
    "convert-cjs": "toesm.cmd --input=\"src/cjs/**/*.cjs\" --output=src/esm/ --input=\"example/**/*.cjs\" --output=example/esm/ --config=\".toesm.js\"",
    "bundle:prod:cjs": "rollup --config rollup-cjs.config.js",
    "bundle:prod:esm": "rollup --config rollup-esm.config.js",
    "bundle:prod": "npm run bundle:prod:cjs && npm run bundle:prod:esm"
  },
  "author": "Patrice Thimothee",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-commonjs": "^21.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.8.0",
    "rollup": "^2.67.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "chalk-cjs": "npm:chalk@^4.1.2",
    "color-convert": "^2.0.1",
    "color-convert-cjs": "npm:color-convert@^2.0.1",
    "rgb-hex": "^4.0.0",
    "rgb-hex-cjs": "npm:rgb-hex@^3.0.0",
    "to-esm": "^1.3.4"
  }
}
